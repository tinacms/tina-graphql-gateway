{
  "name": "@forestryio/graphql-helpers",
  "version": "0.0.12",
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "typings": "dist/index.d.ts",
  "bin": {
    "forestry:serve": "./dist/index.js"
  },
  "scripts": {
    "build": "yarn tsup src/index.ts --format cjs --dts",
    "watch": "yarn tsup src/index.ts --watch --format cjs --dts",
    "tar": "yarn pack --out ../../builds/%s-%v.tgz",
    "serve": "yarn nodemon dist/index.js",
    "test": "jest --passWithNoTests",
    "test-watch": "jest  --passWithNoTests --watch"
  },
  "dependencies": {
    "@types/lodash.kebabcase": "^4.1.6",
    "graphql": "^15.3.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.snakecase": "^4.1.1",
    "lodash.upperfirst": "^4.3.1",
    "tslib": "^1.11.1",
    "tsup": "^3.7.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "url": "https://github.com/forestryio/graphql-demo.git",
    "directory": "packages/graphql"
  },
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.upperfirst": "^4.3.6",
    "jest": "^26.1.0",
    "ts-jest": "^26.1.2"
  }
}
